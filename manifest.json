{
  "name": "devHelper",
  "description": "provides useful dev tool shortcuts",
  "version": "0.0.0.3",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage", "activeTab", "scripting", "tabs", "cookies"],
  "action": {
    "default_popup": "Popup/popup.html"
  },
  "icons": {
    "16": "img/dev.png",
    "32": "img/dev.png",
    "48": "img/dev.png",
    "128": "img/dev.png"
  }
}
